{
  "$jason": {
    "head": {
      "title": "Inspark IoT",
      "actions": {
        "$load": {
          "type": "$agent.inject",
          "options": {
            "id": "$webcontainer",
            "items": [
              {
                "url": "https://opmind.github.io/mobile-app/native.js"
              }
            ]
          }
        }
      },
      "templates": {
        "body": {
          "background": {
            "type": "html",
            "url": "https://opmind.github.io/mobile-app/index.html",
            "action": {
              "trigger": "navigate"
            }
          },
        }
      }
    }
  }
}
