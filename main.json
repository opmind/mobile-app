{
  "$jason": {
    "head": {
      "title": "Inspark IoT",
      "offline": "true",
      "actions": {
        "$foreground": {
          "type": "$reload"
        },
        "load": {
          "type": "$script.include",
          "options": {
            "items": [
              { "url": "https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js" },
              { "text": "alert('script injected!';" },
              { "url": "https://cdnjs.cloudflare.com/ajax/libs/crypto-js/3.1.9-1/crypto-js.min.js" },
              { "url": "https://rawgit.com/mathiasbynens/he/master/he.js" }
            ]
          },
          "success": {
            "type": "$render",
            "options": {
              "data": {
                "raw": "no man&#38;s land",
                "decoded": "{{$root.he.decode('no man&#39;s land')}}",
                "unique": "{{$root._.uniq([1,2,3,3,3,4,4,4,4,5])}}"
              }
            }
          }
        }
      },
      "templates": {
        "body": {
          "sections": [{
            "items": [
              {
                "$load": {
                  "type": "$script.include",
                  "options": {
                    "items": [
                      { "text": "var randomcolor = function() { return '#'+Math.floor(Math.random()*16777215).toString(16); }" }
                    ]
                  },
                  "success": {
                    "trigger": "random"
                  }
                }
              }
            ]
          }]
        },
        "random": {
          "type": "$util.alert",
          "options": {
            "title": "Random color",
            "description": "Generated a color: {{$root.randomcolor()}}"
          }
        }
      }
    },
    "body": {
      "background": {
        "type": "html",
        "url": "https://semnext.inspark.ru/semux/#!/main/dashboard",
        "action": {
          "trigger": "navigate"
        }
      },
      "header": {
        "title": {
          "type": "image",
          "style": {
            "width": "35",
            "height": "35"
          },
          "url": "https://raw.githubusercontent.com/opmind/mobile-app/master/ic_launcher.png"
        }
      },
      "footer": {
        "tabs": {
          "style": {
            "background": "rgba(255,255,255,0.8)",
            "color": "#009efa"
          },
          "items": [
            {
              "image": "https://raw.githubusercontent.com/Jasonette/Twitter-UI-example/master/images/home.png",
              "text": "Home",
              "style": {
                "height": "21"
              },
              "url": "https://raw.githubusercontent.com/opmind/mobile-app/master/main.json"
            },
            {
              "image": "https://img.icons8.com/android/344/combo-chart.png",
              "text": "Charts",
              "style": {
                "height": "21"
              },
              "url": "https://raw.githubusercontent.com/opmind/mobile-app/master/charts.json"
            },
            {
              "image": "https://img.icons8.com/android/344/map.png",
              "text": "Map",
              "style": {
                "height": "21"
              },
              "url": "https://raw.githubusercontent.com/opmind/mobile-app/master/map.json"
            },
            {
              "image": "https://img.icons8.com/material-outlined/344/head-profile.png",
              "text": "Profile",
              "style": {
                "height": "21"
              },
              "url": "https://raw.githubusercontent.com/opmind/mobile-app/master/profile.json"
            }
          ]
        }
      }
    }
  }
}
