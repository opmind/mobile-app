{
  "$jason": {
    "head": {
      "title": "Inspark IoT",
      "description": "Inspark IoT platform",
      "icon": "https://opmind.github.io/mobile-app/ic_launcher.png",
      "offline": "true",
      "actions": {
        "$load": {
          "type": "$agent.inject",
          "options": {
            "id": "$webcontainer",
            "items": [
              {
                "url": "https://opmind.github.io/mobile-app/native.js"
              }
            ]
          }
        },
        "$foreground": {
          "type": "$reload"
        },
        "$pull": {
          "type": "$flush",
          "success": {
            "type": "$reload"
          }
        }
      }
    },
    "body": {
      "background": {
        "type": "html",
        "url": "https://semnext.inspark.ru/semux/#!/main/dashboard",
        "action": {
          "trigger": "navigate"
        }
      },
      "header": {
        "title": {
          "type": "image",
          "style": {
            "width": "35",
            "height": "35"
          },
          "url": "https://raw.githubusercontent.com/opmind/mobile-app/master/ic_launcher.png"
        }
      },
      "layers": [
        {
          "type": "label",
          "text": "ðŸ‘ˆ",
          "style": {
            "bottom": "20",
            "right": "20",
            "background": "#ff0000",
            "color": "#ffffff",
            "width": "100",
            "height": "100",
            "align": "center",
            "size": "30",
            "corner_radius": "50"
          },
          "action": {
            "type": "$back"
          }
        }
      ]
    }
  }
}
