{
  "$jason": {
    "head": {
      "title": "Inspark IoT",
      "actions": {
        "$load": {
          "type": "$agent.inject",
          "options": {
            "id": "$webcontainer",
            "items": [
              {
                "url": "https://opmind.github.io/mobile-app/native.js"
              }
            ]
          }
        },
        "$foreground": {
          "type": "$reload"
        },
        "handleLink": [{
          "{{#if $jason.url.indexOf('dashboard/20') !== -1 }}": {
            "type": "$href",
            "options": {
              "url": "https://semnext.inspark.ru/semux/#!/main/dashboard"
            }
          }
        }, {
          "{{#else}}": {
            "type": "$default"
          }
        }]
      }
    },
    "body": {
      "background": {
        "type": "html",
        "url": "https://semnext.inspark.ru/semux/#!/main/dashboard",
        "action": {
          "trigger": "handleLink"
        }
      },
      "header": {
        "title": {
          "type": "image",
          "style": {
            "width": "35",
            "height": "35"
          },
          "url": "https://raw.githubusercontent.com/opmind/mobile-app/master/ic_launcher.png"
        }
      }
    }
  }
}
